{
  "name": "@matthew-hre/cms",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/types.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "tsx src/example.ts"
  },
  "author": "Matthew Hrehirchuk <matthew_hre@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "zod": "^4.0.15"
  }
}